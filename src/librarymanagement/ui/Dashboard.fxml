<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="256.0" prefWidth="189.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="librarymanagement.ui.SystemController">
   <children>
      <Label fx:id="welcomeLbl" alignment="CENTER" layoutX="2.0" layoutY="1.0" prefHeight="27.0" prefWidth="183.0" text="Welcome">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <FlowPane alignment="TOP_CENTER" layoutX="6.0" layoutY="48.0" orientation="VERTICAL" prefHeight="159.0" prefWidth="183.0" rowValignment="TOP">
         <children>
            <Button fx:id="addNewMember" mnemonicParsing="false" onAction="#addNewLibraryMember" text="Add New Library Member">
               <FlowPane.margin>
                  <Insets top="2.0" />
               </FlowPane.margin>
            </Button>
            <Button fx:id="editMember" mnemonicParsing="false" onAction="#editLibraryMemberInfo" prefHeight="25.0" prefWidth="153.0" text="Edit Library Member Info">
               <FlowPane.margin>
                  <Insets top="4.0" />
               </FlowPane.margin>
            </Button>
            <Button fx:id="addBook" mnemonicParsing="false" onAction="#addBook" prefHeight="25.0" prefWidth="153.0" text="Add Book">
               <FlowPane.margin>
                  <Insets top="4.0" />
               </FlowPane.margin>
            </Button>
            <Button fx:id="checkOutBook" mnemonicParsing="false" onAction="#checkOutBook" prefHeight="25.0" prefWidth="153.0" text="Checkout Book">
               <FlowPane.margin>
                  <Insets top="4.0" />
               </FlowPane.margin>
            </Button>
            <Button fx:id="checkInBook" mnemonicParsing="false" onAction="#checkInBook" prefHeight="25.0" prefWidth="152.0" text="Checkin Book">
               <FlowPane.margin>
                  <Insets top="4.0" />
               </FlowPane.margin>
            </Button>
         </children>
      </FlowPane>
      <Button fx:id="logout" layoutX="10.0" layoutY="217.0" mnemonicParsing="false" onAction="#logout" text="Logout" />
   </children>
</AnchorPane>
